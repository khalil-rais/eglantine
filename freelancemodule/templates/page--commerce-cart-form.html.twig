<header id="header">
	{{ page.header.topheaderblock }}
	<div class="container">
		<div class="row">
			<div class="col-md-4">
				{{ page.header.sitebranding }}
			</div>
			<div class="col-md-8">
				<div class="row">
					<div class="col-md-10 pull-right headercontact">
						{{ page.header.headercontact }}
					</div>
					<div class="col-md-10 pull-right">
						{{ page.header.searchproductblock }}
					</div>
				</div>
		</div>
	</div>
	{{ page.header.mainnavigation }}
</header>
<div id="inner-banner">
	<div class="container">
	  <h1>Bleu De Chanel</h1>
	  <em>We are Parfuma Perfume Store where you can explore all kinds of Perfume</em>
	  <ol class="breadcrumb">
	    <li><a href="#">Home</a></li>
	    <li><a href="#">Shop</a></li>
	    <li class="active">Product  Details</li>
	  </ol>
	</div>
</div>
<div class="container">
  <div class="row">
    <div class="col-md-12 col-lg-12 col-sm-12">
		{{ page.content }}
    </div>
  </div>
</div>




